{
    "name": "codepair",
    "displayName": "CodePair",
    "description": "an extension for pair programming",
    "version": "0.0.1",
    "publisher": "turbo",
    "engines": {
        "vscode": "^1.5.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:extension.sayHello"
    ],
    "main": "./out/src/extension",
    "contributes": {
        "commands": [{
            "command": "extension.sayHello",
            "title": "Hello World"
        },
        {   "command": "extension.send",
            "title": "Connect to Client"
        },
        {   "command": "extension.receive",
            "title": "Connect to Server"
        },
        {   "command": "extension.get",
            "title": "Get Text"
        },
        {   "command": "extension.set",
            "title": "Set Text"
        }]
    },
    "scripts": {
        "vscode:prepublish": "tsc -p ./",
        "compile": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.0.3",
        "vscode": "^1.0.0",
        "mocha": "^2.3.3",
        "@types/node": "^6.0.40",
        "@types/mocha": "^2.2.32"
    }
}